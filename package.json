{
  "name": "project-lee",
  "version": "1.0.0",
  "description": "Digital Designers - Web Technology",
  "main": ".eleventy.js",
  "dependencies": {
    "@11ty/eleventy": "^2.0.1",
    "@11ty/eleventy-img": "^3.1.0",
    "@11ty/eleventy-navigation": "^0.3.5",
    "@11ty/eleventy-plugin-rss": "^1.2.0",
    "@11tyrocks/eleventy-plugin-emoji-readtime": "^1.0.1",
    "npm-run-all": "^4.1.5",
    "sass": "^1.77.8"
  },
  "scripts": {
    "watch:eleventy": "eleventy --serve",
    "watch:sass": "sass --watch --no-source-map src/assets/css:_site/assets",
    "build:eleventy": "eleventy",
    "build:sass": "sass  --no-source-map --style=compressed src/assets/css:_site/assets",
    "start": "npm-run-all build:sass --parallel watch:*",
    "build": "npm-run-all build:*",
    "debug": "DEBUG=* npx eleventy"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kiri-vadivelu/project-lee.git"
  },
  "keywords": [
    "\"Jamstack\"",
    "\"11ty\"",
    "\"SASS\"",
    "\"Jaffna",
    "CSS\"",
    "\"Netlify\""
  ],
  "author": "Kiri Vadivelu <kiri@digitaldesigners.ca>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kiri-vadivelu/project-lee/issues"
  },
  "homepage": "https://github.com/kiri-vadivelu/project-lee#readme"
}
